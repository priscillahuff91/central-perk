    "@types/react-router-dom": "^5.1.5",
    "json-server": "^0.16.1",
    "node-sass": "^7.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-promise-tracker": "^2.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-spinners": "^0.9.0"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "prettier": "2.0.5",
